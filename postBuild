# enable jupyter_server_proxy
/srv/conda/envs/notebook/bin/jupyter-serverextension --log-level=0 --show-config --sys-prefix enable jupyter_server_proxy
# install the bokeh server extension so that
# bokeh launches at startup
mv panelserverextension.py /srv/conda/envs/notebook/lib/python3.10/site-packages/panelserverextension.py
# enable bokeh extension
/srv/conda/envs/notebook/bin/jupyter-serverextension --log-level=0 --show-config --sys-prefix enable panelserverextension
# jupyter nbextension list